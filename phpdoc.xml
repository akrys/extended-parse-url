<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<parser>
		<default-package-name>Extended ParseURL</default-package-name>
		<target>./_phpdoc/_raw_output</target>

		<extensions>
			<extension>php</extension>
		</extensions>
	</parser>
	<transformer>
		<target>./_phpdoc/html/</target>
	</transformer>

	<logging>
		<level>warn</level>
		<paths>
			<default>./_phpdoc/_log/{DATE}.log</default>
			<errors>./_phpdoc/_log/{DATE}.errors.log</errors>
		</paths>
	</logging>
	<transformations>
		<template name="clean" />
	</transformations>
	<files>
		<directory>./*</directory>
		<ignore>*/tests/*</ignore>
		<ignore>*/vendor/*</ignore>
		<ignore>*/_phpunit/*</ignore>
		<ignore>*/_phpdoc/*</ignore>
	</files>
</phpdoc>